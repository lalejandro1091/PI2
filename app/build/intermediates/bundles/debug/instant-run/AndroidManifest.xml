<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alejandro.smatlock"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.alejandro.smatlock.Splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.alejandro.smatlock.Login" />
        <activity android:name="com.example.alejandro.smatlock.Principal" />
        <activity android:name="com.example.alejandro.smatlock.Registrarse" />
        <activity android:name="com.example.alejandro.smatlock.Puertas" />
        <activity android:name="com.example.alejandro.smatlock.Invitar" />
    </application>

</manifest>